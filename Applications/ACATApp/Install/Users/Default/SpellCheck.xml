    <Spelling word="wouldnt" replaceWith="wouldn't"/>
    <Spelling word="id" replaceWith="I'd"/>
    <Spelling word="ive" replaceWith="I've"/>
    <Spelling word="havent" replaceWith="haven't"/>
    <Spelling word="isnt" replaceWith="isn't"/>
  </Spellings>
