    <Widget class="BoxWidget"  name="Box2">
      <Widget class="RowWidget" name="Row5">
        <Widget class="ScannerButton" name="B29" subclass="Select_1" enabled="contextual"/>
        <Widget class="ScannerButton" name="B30" subclass="Select_2" enabled="contextual"/>
        <Widget class="ScannerButton" name="B31" subclass="Select_3" enabled="contextual"/>
        <Widget class="ScannerButton" name="B32" subclass="Select_4" enabled="contextual"/>
        <Widget class="ScannerButton" name="B33" subclass="Select_5" enabled="contextual"/>
        <Widget class="ScannerButton" name="B34" subclass="Select_6" enabled="contextual"/>
        <Widget class="ScannerButton" name="B35"/>
      </Widget>
      
      <Widget class="RowWidget" name="Row6">
        <Widget class="ScannerButton" name="B36" subclass="Select_7" enabled="contextual"/>
        <Widget class="ScannerButton" name="B37" subclass="Select_8" enabled="contextual"/>
        <Widget class="ScannerButton" name="B38" subclass="Select_9" enabled="contextual"/>
        <Widget class="ScannerButton" name="B39" subclass="Select_10" enabled="contextual"/>
        <Widget class="ScannerButton" name="B40"/>
        <Widget class="ScannerButton" name="B41"/>
        <Widget class="ScannerButton" name="B42" subclass="ContextualMenu" enabled="false"/>
      </Widget>
        
      <Widget class="RowWidget" name="Row7">
        <Widget class="ScannerButton" name="B43" subclass="Back" enabled="contextual"/>
        <Widget class="ScannerButton" name="B44" subclass="DeletePreviousWord" enabled="contextual"/>
        <Widget class="ScannerButton" name="B45" subclass="ShiftKey" enabled="contextual"/>
        <Widget class="ScannerButton" name="B46" enabled="false"/>
        <Widget class="ScannerButton" name="B47" enabled="false"/>
        <Widget class="ScannerButton" name="B48" enabled="false"/>
        <Widget class="ScannerButton" name="B49" subclass="FileBrowserToggle" enabled="contextual"/>
      </Widget>
      
      <Widget class="RowWidget" name="Row8">
        <Widget class="ScannerButton" name="B50" subclass="EnterKey" enabled="contextual"/>
        <Widget class="ScannerButton" name="B51"/>
        <Widget class="ScannerButton" name="B52" subclass="ToggleTalkWindow" enabled="contextual" defaultEnabled="true"/>
        <Widget class="ScannerButton" name="B53" enabled="false"/>
        <Widget class="ScannerButton" name="B54" subclass="MouseScanner" enabled="false"/>
        <Widget class="ScannerButton" name="B55" enabled="false"/>
        <Widget class="ScannerButton" name="B56" subclass="ShowMainMenu" enabled="false"/>
      </Widget>
    </Widget>
  </Layout>

  <Animations>
    <Animation name="TopLevelRotation"
               start="true"
               onEnter="highlight(PredictionWordList, false); "
               iterations="@HalfScanIterations">
      <Widget name="Box1" onSelect="transition(RowRotationBox1)"/>
      <Widget name="Box2" onSelect="transition(RowRotationBox2)"/>
    </Animation>

    <Animation name="RowRotationBox1"
               iterations="@RowScanIterations"
               hesitateTime="@HesitateTime"
               onEnd="transition(TopLevelRotation)">
      <Widget name="Row1" onSelect="transition(ButtonRotationBox1)"/>
      <Widget name="Row2" onSelect="transition(ButtonRotationBox1)"/>
      <Widget name="Row3" onSelect="transition(ButtonRotationBox1)"/>
      <Widget name="Row4" onSelect="transition(ButtonRotationBox1)"/>
    </Animation>

    <Animation name="ButtonRotationBox1"
               iterations="@ColumnScanIterations"
               onEnd="transition(RowRotationBox1)"
               hesitateTime="@HesitateTime"
              onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)">
      <Widget name="@SelectedWidget/*"/>
    </Animation>

    <Animation name="RowRotationBox2"
               onBack="transition(TopLevelRotation)"
               hesitateTime="@HesitateTime"
               iterations="@RowScanIterations"
               onEnd="transition(TopLevelRotation)">
      <Widget name="Row5" onSelect="transition(ButtonRotationRow5Box2)" />
      <Widget name="Row6" onSelect="transition(ButtonRotationRow6Box2)"/>
      <Widget name="Row7" onSelect="transition(ButtonRotationRow7Box2)"/>
      <Widget name="Row8" onSelect="transition(ButtonRotationRow8Box2)"/>
    </Animation>

    <Animation name="ButtonRotationRow5Box2"
               iterations="@ColumnScanIterations"
               onEnd="transition(RowRotationBox2)"
               hesitateTime="@HesitateTime">
      <Widget name="B29" onHighlightOn="run(@Highlight_1)" onSelect="run(@Select_1);transition(TopLevelRotation)" />
      <Widget name="B30" onHighlightOn="run(@Highlight_2)" onSelect="run(@Select_2);transition(TopLevelRotation)"/>
      <Widget name="B31" onHighlightOn="run(@Highlight_3)" onSelect="run(@Select_3);transition(TopLevelRotation)"/>
      <Widget name="B32" onHighlightOn="run(@Highlight_4)" onSelect="run(@Select_4);transition(TopLevelRotation)"/>
      <Widget name="B33" onHighlightOn="run(@Highlight_5)" onSelect="run(@Select_5);transition(TopLevelRotation)"/>
      <Widget name="B34" onHighlightOn="run(@Highlight_6)" onSelect="run(@Select_6);transition(TopLevelRotation)"/>
      <Widget name="B35" onHighlightOn="run(@Highlight_off)" onSelect="actuate(B35)"/>
    </Animation>

    <Animation name="ButtonRotationRow6Box2"
                   iterations="@ColumnScanIterations"
                   onEnd="transition(RowRotationBox2)"
                   hesitateTime="@HesitateTime">
      <Widget name="B36" onHighlightOn="run(@Highlight_7)" onSelect="run(@Select_7);transition(TopLevelRotation)"/>
      <Widget name="B37" onHighlightOn="run(@Highlight_8)" onSelect="run(@Select_8);transition(TopLevelRotation)"/>
      <Widget name="B38" onHighlightOn="run(@Highlight_9)" onSelect="run(@Select_9);transition(TopLevelRotation)"/>
      <Widget name="B39" onHighlightOn="run(@Highlight_10)" onSelect="run(@Select_10);transition(TopLevelRotation)"/>
      <Widget name="B40" onHighlightOn="run(@Highlight_off)" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
      <Widget name="B41" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
      <Widget name="B42" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
    </Animation>

    <Animation name="ButtonRotationRow7Box2"
                       iterations="@ColumnScanIterations"
                       onEnd="transition(RowRotationBox2)"
                       hesitateTime="@HesitateTime"
                      onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)">
      <Widget name="B43"/>
      <Widget name="B44"/>
      <Widget name="B45"/>
      <Widget name="B46"/>
      <Widget name="B47"/>
      <Widget name="B48"/>
      <Widget name="B49" onSelect="actuate(@SelectedWidget);transition(TopLevelRotation)" />
    </Animation>

    <Animation name="ButtonRotationRow8Box2"
                           iterations="@ColumnScanIterations"
                           onEnd="transition(RowRotationBox2)"
                           hesitateTime="@HesitateTime"         >
      <Widget name="B50" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
      <Widget name="B51" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
      <Widget name="B52" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
      <Widget name="B53" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
      <Widget name="B54" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
      <Widget name="B55" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
      <Widget name="B56" onSelect="actuate(@SelectedWidget); transition(TopLevelRotation)"/>
    </Animation>

    <Animation name="ButtonPressed"
       onEnter=""
       iterations="1"
       onEnd="transition(RowRotationBox2)">
      <Widget name="@SelectedWidget" hesitateTime="@FirstRepeatTime" onSelect="actuate(@SelectedWidget);transition(ButtonPressed)" />
    </Animation>
  </Animations>
</ACAT>

